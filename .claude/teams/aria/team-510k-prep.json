{
  "schema_version": "1.0",
  "team_id": "aria-510k-prep",
  "team_name": "510(k) Submission Preparation Team",
  "description": "FDA 510(k) submission preparation with parallel section development",
  "version": "1.0.0",
  "created": "2026-02-09",
  "complexity_threshold": {
    "min_sections": 5,
    "activation_mode": "auto"
  },
  "teammates": [
    {
      "name": "fda-researcher",
      "agent_type": "team-researcher",
      "model": "haiku",
      "role": "FDA Regulatory Researcher",
      "description": "Research FDA 510(k) requirements, predicate devices, and substantial equivalence criteria",
      "permissions": {
        "mode": "plan",
        "tools": ["Read", "Grep", "Glob", "WebSearch", "WebFetch"]
      },
      "responsibilities": [
        "Research predicate device 510(k) summaries",
        "Identify applicable FDA guidance documents",
        "Document substantial equivalence criteria",
        "Research device classification and regulatory pathways"
      ],
      "file_ownership": [
        ".aria/510k/research/**",
        ".aria/510k/predicate-devices/**"
      ],
      "skills": [
        "aria-knowledge-fda",
        "aria-submission-templates"
      ]
    },
    {
      "name": "predicate-analyst",
      "agent_type": "team-analyst",
      "model": "sonnet",
      "role": "Predicate Device Analyst",
      "description": "Analyze device specifications and predicate device comparisons for substantial equivalence",
      "permissions": {
        "mode": "plan",
        "tools": ["Read", "Grep", "Glob", "Write", "Edit"]
      },
      "responsibilities": [
        "Compare device specifications to predicate",
        "Analyze technological characteristics",
        "Identify differences and justify safety/effectiveness",
        "Document substantial equivalence rationale"
      ],
      "file_ownership": [
        ".aria/510k/comparison/**",
        ".aria/510k/technological-characteristics/**"
      ],
      "skills": [
        "aria-domain-raqa",
        "aria-submission-templates"
      ]
    },
    {
      "name": "submission-writer",
      "agent_type": "team-writer",
      "model": "sonnet",
      "role": "Submission Writer",
      "description": "Draft 510(k) submission sections following FDA eCopy templates",
      "permissions": {
        "mode": "acceptEdits",
        "tools": ["Read", "Write", "Edit", "Grep"]
      },
      "responsibilities": [
        "Draft Indications for Use section",
        "Prepare Device Description section",
        "Compile Substantial Equivalence summary",
        "Format per FDA eCopy requirements"
      ],
      "file_ownership": [
        ".aria/510k/submission/**",
        ".aria/510k/sections/**"
      ],
      "skills": [
        "aria-submission-templates",
        "aria-document-quality"
      ]
    }
  ],
  "workflow": {
    "phases": [
      {
        "phase": "research",
        "teammates": ["fda-researcher"],
        "parallel": true,
        "description": "Research FDA requirements and predicate devices"
      },
      {
        "phase": "analysis",
        "teammates": ["predicate-analyst"],
        "parallel": true,
        "depends_on": ["research"],
        "description": "Analyze device specifications and comparisons"
      },
      {
        "phase": "drafting",
        "teammates": ["submission-writer"],
        "parallel": false,
        "depends_on": ["analysis"],
        "description": "Draft submission sections"
      },
      {
        "phase": "review",
        "teammates": ["fda-researcher", "predicate-analyst", "submission-writer"],
        "parallel": false,
        "depends_on": ["drafting"],
        "description": "Cross-reference consistency review"
      }
    ],
    "task_board": {
      "enabled": true,
      "shared_tasks": [
        {
          "id": "research-predicate",
          "subject": "Research predicate device 510(k) summaries",
          "assignee": "fda-researcher",
          "priority": "high"
        },
        {
          "id": "identify-guidance",
          "subject": "Identify applicable FDA guidance documents",
          "assignee": "fda-researcher",
          "priority": "high",
          "depends_on": ["research-predicate"]
        },
        {
          "id": "analyze-specs",
          "subject": "Analyze device specifications vs predicate",
          "assignee": "predicate-analyst",
          "priority": "high",
          "depends_on": ["research-predicate"]
        },
        {
          "id": "compare-tech",
          "subject": "Compare technological characteristics",
          "assignee": "predicate-analyst",
          "priority": "high",
          "depends_on": ["analyze-specs"]
        },
        {
          "id": "draft-indications",
          "subject": "Draft Indications for Use section",
          "assignee": "submission-writer",
          "priority": "high",
          "depends_on": ["compare-tech"]
        },
        {
          "id": "draft-description",
          "subject": "Draft Device Description section",
          "assignee": "submission-writer",
          "priority": "high",
          "depends_on": ["compare-tech"]
        },
        {
          "id": "draft-equivalence",
          "subject": "Draft Substantial Equivalence summary",
          "assignee": "submission-writer",
          "priority": "high",
          "depends_on": ["draft-indications", "draft-description"]
        },
        {
          "id": "format-ecopy",
          "subject": "Format submission per FDA eCopy requirements",
          "assignee": "submission-writer",
          "priority": "medium",
          "depends_on": ["draft-equivalence"]
        },
        {
          "id": "cross-reference-review",
          "subject": "Review cross-reference consistency across sections",
          "assignee": "submission-writer",
          "priority": "high",
          "depends_on": ["format-ecopy"]
        }
      ]
    }
  },
  "coordination_patterns": {
    "predicate_device_change": {
      "trigger": "predicate_device_selection_changes",
      "action": "notify_all_members",
      "flagged_sections": [
        "technological-characteristics",
        "substantial-equivalence",
        "indications-for-use",
        "device-description"
      ]
    },
    "section_completion": {
      "trigger": "section_completed",
      "action": "notify_dependent_sections",
      "update_shared_board": true
    },
    "cross_reference_check": {
      "trigger": "before_finalization",
      "action": "validate_all_cross_references",
      "participants": ["predicate-analyst", "submission-writer"]
    }
  },
  "escalation_paths": [
    {
      "condition": "Substantial equivalence unclear",
      "escalate_to": "regulatory-strategy",
      "action": "Request regulatory pathway reassessment"
    },
    {
      "condition": "New predicate device needed",
      "escalate_to": "regulatory-affairs",
      "action": "Request alternative predicate device search"
    },
    {
      "condition": "Technological characteristics require engineering input",
      "escalate_to": "engineering-lead",
      "action": "Request technical specifications verification"
    },
    {
      "condition": "eCopy formatting issues",
      "escalate_to": "submission-specialist",
      "action": "Request formatting guidance"
    }
  ],
  "quality_gates": {
    "predicate_device_identified": true,
    "substantial_equivalence_documented": true,
    "all_sections_complete": true,
    "cross_references_consistent": true,
    "ecopy_format_compliant": true,
    "regulatory_citations_verified": true
  },
  "file_ownership_boundaries": {
    "fda-researcher": [
      ".aria/510k/research/**",
      ".aria/510k/predicate-devices/**",
      ".aria/510k/guidance/**"
    ],
    "predicate-analyst": [
      ".aria/510k/comparison/**",
      ".aria/510k/technological-characteristics/**",
      ".aria/510k/differences/**"
    ],
    "submission-writer": [
      ".aria/510k/submission/**",
      ".aria/510k/sections/**",
      ".aria/510k/ecopy/**"
    ]
  },
  "fallback_strategy": {
    "enabled": true,
    "mode": "sequential",
    "fallback_agents": [
      "aria-orchestrator",
      "expert-submission",
      "expert-regulatory"
    ],
    "resume_point": "last_completed_section"
  },
  "completion_markers": {
    "all_sections_complete": true,
    "cross_references_validated": true,
    "ecopy_format_verified": true,
    "regulatory_review_approved": true
  }
}
